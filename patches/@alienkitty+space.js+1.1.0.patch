+        this.initLights();
+        this.initLoaders();
+        this.initEnvironment();
+        this.initControls();
+        this.initPhysics();
+
+        this.addListeners();
+    }
+
+    static initWorld() {
+        this.renderer = new WebGLRenderer({
+            powerPreference: 'high-performance',
+            stencil: false,
+            antialias: true,
+            // alpha: true
+        });
