+    constructor(mesh) {
+        super();
+
+        this.mesh = mesh;
+
+        this.initPanel();
+    }
+
+    initPanel() {
+        const mesh = this.mesh;
+        const point = Point3D.getPoint(mesh);
+
+        // Defaults
+        if (!mesh.userData.normals) {
+            mesh.userData.normals = false;
