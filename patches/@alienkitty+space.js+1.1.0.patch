+                {
+                    type: 'list',
+                    label: 'UV',
+                    list: UVHelperOptions,
+                    value: getKeyByValue(UVHelperOptions, mesh.userData.uv),
+                    callback: value => {
+                        mesh.userData.uv = UVHelperOptions[value];
+                        point.toggleUVHelper(mesh.userData.uv);
+                    }
+                }
+                // TODO: Texture thumbnails
+            );
+        }
+
+        items.forEach(data => {
+            this.add(new PanelItem(data));
+        });
+    }
