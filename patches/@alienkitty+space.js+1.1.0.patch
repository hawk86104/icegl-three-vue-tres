+    }
+
+    initPanel() {
+        const mesh = this.mesh;
+
+        // Defaults
+        if (!mesh.userData.subsurface) {
+            mesh.userData.subsurface = false;
+
+            mesh.userData.subsurfaceUniforms = {
+                thicknessDistortion: { value: 0.1 },
