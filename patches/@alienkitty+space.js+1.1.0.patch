+
+        const DEBUG = /[?&]debug/.test(location.search);
+
+        const amount = parseInt(location.search.slice(1), 10) || 3;
+        const count = Math.pow(amount, 3);
+
+        const color = new Color();
+
+        // init
+
+        const renderer = new WebGLRenderer({ antialias: true });
