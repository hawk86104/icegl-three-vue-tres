+            }
+
+            removeListeners() {
+                ticker.remove(this.onUpdate);
+            }
+
+            // Event handlers
+
+            onUpdate = () => {
+                if (this.needsUpdate) {
+                    this.update();
+                }
+            };
+
+            onProgress = ({ progress }) => {
+                clearTween(this);
+
+                this.needsUpdate = true;
+
+                tween(this, { progress }, 500, 'easeOutCubic', () => {
+                    this.needsUpdate = false;
+
+                    if (this.progress >= 1) {
+                        this.onComplete();
+                    }
+                });
+            };
+
+            onComplete = () => {
+                this.removeListeners();
+
